<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>LYC</display-name>
  <welcome-file-list>
    <welcome-file>desktop_web/frontend/home/Home.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Sign Up</servlet-name>
    <servlet-class>com.lyc.controller.HomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Sign Up</servlet-name>
    <url-pattern>/SignUp.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Export PDF</servlet-name>
    <servlet-class>com.lyc.controller.ExportPDFServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Export PDF</servlet-name>
    <url-pattern>/ExportPDF.do</url-pattern>
  </servlet-mapping>
</web-app>